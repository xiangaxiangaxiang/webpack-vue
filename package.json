{
    "version": "1.0.0",
    "description": "My webpack project",
    "name": "my-webpack-project",
    "scripts": {
        "ts-check": "vue-tsc --noEmit",
        "build": "npm run ts-check && cross-env NODE_ENV=production webpack --mode=production --node-env=production",
        "watch": "cross-env NODE_ENV=development webpack --watch",
        "serve": "cross-env NODE_ENV=development webpack serve",
        "preview": "http-server ./dist -p 3000",
        "eslint": "npx eslint ./src --ext .vue,.js,.ts",
        "eslint:fix": "npx eslint ./src --ext .vue,.js,.ts --fix",
        "stylelint": "npx stylelint \"src/**/*.vue\" \"src/**/*.less\"",
        "stylelint:fix": "npx stylelint --fix \"src/**/*.vue\" \"src/**/*.less\"",
        "pre-commit": "npm run ts-check && lint-staged",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@types/node": "^18.11.18",
        "@types/webpack": "^5.28.0",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "@vue/babel-plugin-jsx": "^1.1.1",
        "@vue/babel-preset-jsx": "^1.4.0",
        "babel-loader": "^9.1.2",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.3",
        "eslint": "^8.33.0",
        "eslint-plugin-vue": "^9.9.0",
        "eslint-webpack-plugin": "^4.0.0",
        "html-webpack-plugin": "^5.5.0",
        "http-server": "^14.1.1",
        "husky": "^8.0.3",
        "less": "^4.1.3",
        "less-loader": "^11.1.0",
        "lint-staged": "^13.1.1",
        "mini-css-extract-plugin": "^2.7.2",
        "postcss-html": "^1.5.0",
        "postcss-less": "^6.0.0",
        "postcss-loader": "^7.0.2",
        "postcss-preset-env": "^8.0.1",
        "speed-measure-webpack-plugin": "^1.5.0",
        "style-loader": "^3.3.1",
        "stylelint": "^14.16.1",
        "stylelint-config-prettier": "^9.0.4",
        "stylelint-config-recommended-vue": "^1.4.0",
        "stylelint-config-standard": "^29.0.0",
        "thread-loader": "^3.0.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4",
        "vue-loader": "^17.0.1",
        "vue-tsc": "^1.1.0",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1",
        "workbox-webpack-plugin": "^6.5.4"
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.0.10",
        "element-plus": "^2.2.30",
        "pinia": "^2.0.28",
        "vue": "^3.2.45",
        "vue-router": "^4.1.6"
    },
    "lint-staged": {
        "*.{js,ts,vue}": [
            "npx eslint --fix"
        ],
        "*.{less,vue}": [
            "npx stylelint --fix"
        ]
    },
    "main": "./src/main.ts",
    "repository": "git@github.com:xiangaxiangaxiang/webpack-vue.git",
    "author": "cpx <872632899@qq.com>",
    "license": "MIT"
}
